<Page x:Class="GAIManagment.ModuleMainMenu.Presentation.Pages.MainMenuPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:GAIManagment.ModuleMainMenu.Presentation.Pages"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="MainMenuPage">

    <Grid>
        <StackPanel Margin="20">
            <Button Content="Водители" Click="DriversButton_Click" Height="30" Margin="0,5"/>
            <Button Content="Удостоверения" Click="LicensesButton_Click" Height="30" Margin="0,5"/>

            <Button Content="Статистика выданных удостоверений" Click="GivenLicensesStatistics_Click" Height="30" Margin="0,5"/>
            <Button Content="Статистика изъятых удостоверений" Click="TakenLicensesStatistics_Click" Height="30" Margin="0,5"/>
        </StackPanel>
    </Grid>
</Page>
